<h1>Listing programmes</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Title</th>
      <th>Details</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upcomings.each do |upcoming| %>
     
      <tr>
        <td><%= upcoming.date %></td>
        <td><%= upcoming.title %></td>
        <td><%= upcoming.details %></td>
        <td><%= link_to 'Show', upcoming %></td>
        
      </tr>
     
    <% end %>
  </tbody>
</table>
