<p id="notice"><%= notice %></p>
<h1> Newsletters </h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Name</th>
      
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>  
    
    <% @newsletts.each do |newslett| %>
      
        <tr>
        <td><%= newslett.date.year %></td>
        <td><a href= "<%= newslett.newltr_url%>" display:block download> <%= newslett.name %></a></td>
        </tr>   
      
    <% end %>
  </tbody>
</table>

